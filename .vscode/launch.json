  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug cpp file",
      "type": "cppdbg",
      "request": "launch",
      "program": "E:\\c++\\allpossible.exe",
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": true,
      "MIMode": "gdb",
      "miDebuggerPath": "C:\\Program Files\\MinGW\\bin\\gdb32.exe",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "g++.exe build active file"
    }
  ]
}
